<?xml version="1.0" encoding="UTF-8"?><!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd"><mapper namespace="com.vitamincode.vitamincode_be.mapper.ClassesMapper">    <resultMap id="ClassesResultMap" type="com.vitamincode.vitamincode_be.entity.Classes">        <id column="class_id" property="classId"></id>        <result column="class_name" property="className"></result>        <result column="class_description" property="classDescription"></result>    </resultMap>    <select id="findAllClasses" resultMap="ClassesResultMap">        SELECT class_id, class_name, class_description        FROM classes    </select>    <select id="findClassById" resultMap="ClassesResultMap">        SELECT class_id, class_name, class_description        FROM classes        WHERE class_id = #{classId}    </select></mapper>