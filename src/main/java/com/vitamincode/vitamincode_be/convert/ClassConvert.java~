package com.vitamincode.vitamincode_be.convert;import com.vitamincode.vitamincode_be.dto.response.ClassDtoResponse;import com.vitamincode.vitamincode_be.entity.Classes;import java.util.List;import java.util.stream.Collectors;public class ClassConvert {    public static ClassDtoResponse classEntityConvertToClassDtoResponse(Classes classesEntity) {        return ClassDtoResponse.builder()                .classId(classesEntity.getClassId())                .className(classesEntity.getClassName())                .classDescription(classesEntity.getClassDescription())                .build();    }    public static List<ClassDtoResponse> classEntityConvertListToClassDtoResponse(List<Classes> classesEntity) {        return classesEntity                .stream()                .map(ClassConvert::classEntityConvertToClassDtoResponse)                .collect(Collectors.toList());    }}